    "body-parser": "^1.18.3",
    "bson": "^3.0.0",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "isomorphic-unfetch": "^2.0.0",
    "jquery": "^3.3.1",
    "kerberos": "0.0.24",
