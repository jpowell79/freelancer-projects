    "supertest": "^3.1.0"
  },
  "dependencies": {
    "@zeit/next-css": "^0.2.0",
    "@zeit/next-sass": "^0.2.0",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
