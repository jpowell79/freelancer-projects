    "body-parser": "^1.18.3",
    "bson": "^3.0.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "glob": "^7.1.2",
    "helmet": "^3.13.0",
    "html-escape": "^2.0.0",
    "jquery": "^3.3.1",
