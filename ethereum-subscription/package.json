  },
  "scripts": {
    "test client": "jest --env=jsdom",
    "test client coverage": "jest --env=jsdom --coverage",
    "test server": "mocha server/tests/**/*.test.js --exit",
    "test root": "mocha tests/**/*.test.js --exit",
    "default database": "node scripts.js defaultDatabase",
    "dummy database": "node scripts.js dummyDatabase",
    "remove database": "node scripts.js removeDatabase",
    "dev": "nodemon -w server -w package.json server/index.js",
    "dev debug": "set DEBUG=* & nodemon -w server -w package.json server/index.js",
    "build": "next build client",
    "start": "node server/index.js production NODE_ENV=production"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/client/__tests__/settings.js",
    "collectCoverageFrom": [
      "client/components/**/*.js",
      "client/redux/**/*.js",
      "client/services/**/*.js",
      "client/modules/**/*.js",
      "client/site-modules/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/client/__tests__/settings.js",
      "<rootDir>/client/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/server/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/client/__mocks__/styleMock.js"
    }
  }
}
